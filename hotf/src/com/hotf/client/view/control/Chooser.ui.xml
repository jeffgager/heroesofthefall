<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui' xmlns:o="urn:import:com.sun.org.apache.xpath.internal.operations">

	<ui:style>
		.mainPanel {
			display: block;
		}
		.view {
			cursor: pointer;
			white-space: nowrap;
			text-decoration: underline;
			color: #618bac;
			background-color: #E8E8E8;
		}
		.suggestBox {
			color: #618bac;
			background-color: #E8E8E8;
			border: 1px inset #618bac;
		}
	</ui:style>
	
	<g:FlowPanel addStyleNames="{style.mainPanel}" >
		<g:Label visible="false" ui:field="viewField" addStyleNames="{style.view}">Click to search</g:Label>
		<g:SuggestBox ui:field="suggestBoxField" addStyleNames="{style.suggestBox}">Type to search</g:SuggestBox>
	</g:FlowPanel>

</ui:UiBinder>